        "class Student:\n",
        "    def __init__(self, grades):\n",
        "        self._grades = grades\n",
        "\n",
        "    def get_grade(self, subject):\n",
        "        return self._grades.get(subject)\n",
        "\n",
        "    def set_grade(self, subject, grade):\n",
        "        self._grades[subject] = grade"
