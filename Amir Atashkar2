        "class Point:\n",
        "    def __init__(self, x, y):\n",
        "        self.x = x\n",
        "        self.y = y\n",
        "\n",
        "    def __lt__(self, other):\n",
        "        return (self.x ** 2 + self.y ** 2) < (other.x ** 2 + other.y ** 2)\n",
        "\n",
        "point1 = Point(1, 2)\n",
        "point2 = Point(3, 4)\n",
        "\n",
        "print(point1 < point2)"
      ],
