        "class University:\n",
        "    def teach(self):\n",
        "        print(\"The university teaches students.\")\n",
        "\n",
        "class Department(University):\n",
        "    def research(self):\n",
        "        print(\"The department conducts research.\")\n",
        "\n",
        "class ComputerScience(Department):\n",
        "    def code(self):\n",
        "        print(\"The computer science department writes code.\")"
