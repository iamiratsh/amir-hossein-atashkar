        "class Parent1:\n",
        "    def __init__(self):\n",
        "        self.name = \"Parent1\"\n",
        "\n",
        "class Parent2:\n",
        "    def __init__(self):\n",
        "        self.name = \"Parent2\"\n",
        "\n",
        "class Child(Parent1, Parent2):\n",
        "    def __init__(self):\n",
        "        super().__init__()\n",
        "        print(self.name)"
