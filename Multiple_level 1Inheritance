        "class Animal:\n",
        "    def sound(self):\n",
        "        print(\"The animal makes a sound.\")\n",
        "\n",
        "class Mammal(Animal):\n",
        "    def feed_young(self):\n",
        "        print(\"The mammal feeds its young.\")\n",
        "\n",
        "class Dog(Mammal):\n",
        "    def bark(self):\n",
        "        print(\"The dog barks.\")"
