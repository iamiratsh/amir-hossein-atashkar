        "from abc import ABC, abstractmethod\n",
        "\n",
        "class Abstraction(ABC):\n",
        "    @abstractmethod\n",
        "    def do_something(self):\n",
        "        pass\n",
        "\n",
        "class ConcreteClass(Abstraction):\n",
        "    def do_something(self):\n",
        "        print(\"Doing something in ConcreteClass\")\n",
        "\n",
        "obj = ConcreteClass()\n",
        "obj.do_something()"
      ],
