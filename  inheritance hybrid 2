        "\n",
        "class Animal:\n",
        "    def sound(self):\n",
        "        print(\"The animal makes a sound\")\n",
        "\n",
        "# Parent class\n",
        "class Mammal(Animal):\n",
        "    def eat(self):\n",
        "        print(\"The mammal eats\")\n",
        "\n",
        "# Child class\n",
        "class Dog(Mammal):\n",
        "    def sound(self):\n",
        "        print(\"The dog barks\")\n"
