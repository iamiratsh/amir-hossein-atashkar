      "cell_type": "code",
      "source": [
        "# Parent class 1\n",
        "class Flyer:\n",
        "    def fly(self):\n",
        "        print(\"The flyer flies\")\n",
        "\n",
        "# Parent class 2\n",
        "class Walker:\n",
        "    def walk(self):\n",
        "        print(\"The walker walks\")\n",
        "\n",
        "# Child class 1\n",
        "class Bird(Flyer, Walker):\n",
        "    def chirp(self):\n",
        "        print(\"The bird chirps\")\n",
        "\n",
        "# Child class 2\n",
        "class Parrot(Bird):\n",
        "    def talk(self):\n",
        "        print(\"The parrot talks\")\n"
      ],
      "metadata": {
        "id": "Jg7oUxH_lbVd"
